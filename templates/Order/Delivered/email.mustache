{{!
    Sample of data
    {
        "first_name": "Ben",
        "last_name": "Brown",
        "birth_date": "1999-09-09",
        "dispensed_date": "04-09-2021",
        "shipped_date": "04-09-2021",
        "invoice_number": 12345,
        "tracking_number": "1234123124123",
        "tracking_url": "http://www.gmail.com",
        "short_tracking_url": "http://www.gmail.com",
        "invoice_url": "http://www.gmail.com",
        "short_invoice_url": "http://www.gmail.com",
        "count_filled": 1,
        "multiple_filled": true,
        "filled": {
            "rxs": [
                {"name": "drug 1"},
                {"name": "drug 2"}
            ]
        },
        "no_fill": {
            "rxs": [
                {"name": "no drug 1"},
                {"name": "no drug 2"}
            ]
        }
    }
}}
<html>
    <body>
        <p>Hello {{first_name}},</p>
        <p>Your Good Pill order of {{count_filled}} item{{#multiple_filled}}s{{/multiple_filled}} has been delivered.</p>
        {{#filled}}
        <p>
            <u>This order includes:</u>
            <ul>
                {{#rxs}}
                    <li>{{name}}</li>
                {{/rxs}}
            </ul>
        </p>
        {{/filled}}
        <p>Thanks!<br>
        The Good Pill Team</p>
        {{#nofill}}
        <p>
        <u>We could not fill these items:</u>
        <ul>
            {{#rxs}}
                <li>{{name}}</li>
            {{/rxs}}
        </ul>
        <p>
        {{/nofill}}
    </body>
</html>
